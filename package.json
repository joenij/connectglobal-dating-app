{
  "name": "connectglobal-dating-app",
  "version": "1.0.0",
  "description": "Global dating app with cultural exchange and fair pricing",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/(services|components|utils)",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:ci": "jest --coverage --watchAll=false --verbose",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:dev": "cd android && ./gradlew assembleDebug -PBUILD_ENV=development",
    "build:android:test": "cd android && ./gradlew assembleDebug -PBUILD_ENV=test",
    "build:android:prod": "cd android && ./gradlew assembleRelease -PBUILD_ENV=production",
    "build:ios": "cd ios && xcodebuild -workspace ConnectGlobal.xcworkspace -scheme ConnectGlobal -configuration Release archive",
    "build:ios:dev": "cd ios && xcodebuild -workspace ConnectGlobal.xcworkspace -scheme ConnectGlobal -configuration Debug",
    "build:ios:test": "cd ios && xcodebuild -workspace ConnectGlobal.xcworkspace -scheme ConnectGlobal -configuration Test",
    "build:ios:prod": "cd ios && xcodebuild -workspace ConnectGlobal.xcworkspace -scheme ConnectGlobal -configuration Release",
    "backend:dev": "echo 'Development server runs on https://dev.jneconnect.com - Check server status'",
    "backend:test": "echo 'Test server runs on https://test.jneconnect.com - Check server status'", 
    "backend:prod": "echo 'Production server runs on https://api.jneconnect.com - Check server status'",
    "backend:start": "cd backend && npm start",
    "backend:test:watch": "cd backend && npm run test:watch",
    "backend:test:coverage": "cd backend && npm run test:coverage",
    "dev:setup": "node setup-open-source.js",
    "dev:start": "scripts\\start-dev.bat",
    "dev:stop": "docker-compose down",
    "dev:logs": "docker-compose logs -f",
    "setup:minio": "node scripts\\setup-minio.js",
    "db:migrate": "echo Run schema in Supabase Studio at http://localhost:3001",
    "services:status": "docker-compose ps",
    "test:all": "npm run test:ci && npm run backend:test:coverage"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@stripe/stripe-react-native": "^0.35.0",
    "@supabase/supabase-js": "^2.52.0",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-elements": "^3.4.3",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-google-mobile-ads": "^13.6.1",
    "react-native-localization": "^2.3.2",
    "react-native-permissions": "^4.0.4",
    "react-native-vector-icons": "^10.0.3",
    "react-native-video": "^5.2.1",
    "react-redux": "^8.1.3",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.3",
    "jest-html-reporters": "^3.1.5",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "dating",
    "global",
    "cultural-exchange",
    "react-native",
    "mobile-app"
  ],
  "author": "ConnectGlobal Team",
  "license": "MIT",
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.ts",
      "@testing-library/jest-native/extend-expect"
    ]
  }
}

{
  "name": "connectglobal-dating-app",
  "version": "1.0.0",
  "description": "Global dating app with cultural exchange and fair pricing",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ConnectGlobal.xcworkspace -scheme ConnectGlobal -configuration Release archive",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "dev:setup": "node setup-open-source.js",
    "dev:start": "scripts\\start-dev.bat",
    "dev:stop": "docker-compose down",
    "dev:logs": "docker-compose logs -f",
    "setup:minio": "node scripts\\setup-minio.js",
    "db:migrate": "echo Run schema in Supabase Studio at http://localhost:3001",
    "services:status": "docker-compose ps"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@stripe/stripe-react-native": "^0.35.0",
    "@supabase/supabase-js": "^2.52.0",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-biometrics": "^3.0.1",
    "react-native-elements": "^3.4.3",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-i18n": "^2.0.15",
    "react-native-permissions": "^4.0.4",
    "react-native-vector-icons": "^10.0.3",
    "react-native-video": "^5.2.1",
    "react-native-vision-camera": "^3.6.4",
    "react-redux": "^8.1.3",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "dating",
    "global",
    "cultural-exchange",
    "react-native",
    "mobile-app"
  ],
  "author": "ConnectGlobal Team",
  "license": "MIT"
}

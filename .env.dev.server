# Development Server Environment Configuration for jneconnect.com
NODE_ENV=development
API_VERSION=v1
PORT=8001

# Development Database Configuration - PostgreSQL on same server
DB_HOST=127.0.0.1
DB_PORT=5432
DB_NAME=connectglobal_dev
DB_USER=connectglobal_user
DB_PASSWORD=ConnectGlobal2024!Secure#DB
DB_SSL=false

# Development Environment Features
TEST_DATA_ENABLED=false
ENABLE_DEBUG_FEATURES=true

# JWT Configuration  
JWT_SECRET=dev-jwt-secret-key-for-development-only
JWT_EXPIRES_IN=7d

# CORS Configuration for development
CORS_ORIGIN=*

# Development Logging
LOG_LEVEL=debug
